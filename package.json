  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/identity-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "identity",
    "srv"
  ],
  "dependencies": {
    "@restorecommerce/acs-client": "^1.4.8",
    "@restorecommerce/chassis-srv": "^1.4.6",
    "@restorecommerce/grpc-client": "^2.1.0",
    "@restorecommerce/kafka-client": "^1.1.17",
    "@restorecommerce/logger": "^1.2.8",
    "@restorecommerce/rc-grpc-clients": "^5.1.17",
    "@restorecommerce/resource-base-interface": "^1.4.4",
    "@restorecommerce/scs-jobs": "^0.1.15",
    "@restorecommerce/service-config": "^1.0.11",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-de": "^3.0.2",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "@zxcvbn-ts/matcher-pwned": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.14.2",
    "cross-env": "^7.0.3",
    "jose": "^5.2.0",
    "lodash-es": "^4.17.21",
    "nice-grpc-server-reflection": "^2.0.9",
    "node-fetch": "^3.3.2",
    "oauth": "^0.10.0",
    "otplib": "^12.0.1",
    "redis": "^4.6.12",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.1.8",
    "@restorecommerce/dev": "^0.0.6",
    "@restorecommerce/protos": "^6.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.10",
    "@types/oauth": "^0.9.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "c8": "^9.1.0",
    "coveralls": "^3.1.1",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "should": "^13.2.3",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@grpc/grpc-js": "^1.9.11"
  },
  "scripts": {
    "start": "node lib/start.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec node --loader ts-node/esm ./src/start.ts",
    "test": "npm run lint && c8 --reporter=text npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha --full-trace --exit --timeout 30000",
    "test-debug": "npm run mocha -- --inspect-brk",
    "lcov-report": "c8 report --reporter=lcov",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "build:tsc": "esbuild ./src/start.ts --bundle --platform=node --outfile=lib/start.js --minify --tree-shaking=true --sourcemap=inline",
    "build:jobs": "find ./src/jobs -maxdepth 1 -type f -exec bash -c 'esbuild {} --bundle --platform=node --outfile=lib/jobs/$(basename {} .ts).js --minify --tree-shaking=true --sourcemap=inline' \\;",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc build:jobs"
  },
  "engines": {
    "node": ">= 20.8.0"
  },
  "type": "module"
}
