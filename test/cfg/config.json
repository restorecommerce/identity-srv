          ]
        },
        "update": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "upsert": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-role-srv": {
        "read": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "create": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "update": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "upsert": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "delete": {
          "transport": [
