{
  "service": {
    "userActivationRequired": true,
    "register": true
  },
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "error",
      "colorize": true,
      "prettyPrint": true
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0
    }
  },
  "client": {
    "hbs_templates": {
      "url": true,
      "prefix": "http://localhost:8080/",
      "templates": {
        "subject": "restore_identity/subject.hbs",
        "body_register": "restore_identity/body_register.hbs",
        "body_change": "restore_identity/body_change.hbs",
        "layout": "restore_identity/layout.hbs"
      }
    },
    "service-user": {
      "transports": {
        "grpc": {
          "service": "io.restorecommerce.user.Service",
          "protos": [
            "io/restorecommerce/user.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50051"
        ]
      },
      "endpoints": {
        "activate": {},
        "changePassword": {},
        "changeEmailId": {},
        "find": {},
        "register": {},
        "unregister": {},
        "login": {},
        "create": {},
        "read": {},
        "delete": {},
        "update": {}
      }
    },
    "service-role": {
      "transports": {
        "grpc": {
          "service": "io.restorecommerce.role.Service",
          "protos": [
            "io/restorecommerce/role.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50051"
        ]
      },
      "endpoints": {
        "read": {},
        "create": {},
        "delete": {},
        "update": {},
        "upsert": {}
      }
    },
    "service-reflection": {
      "transports": {
        "grpc": {
          "service": "grpc.reflection.v1alpha.ServerReflection",
          "protos": [
            "io/restorecommerce/user.proto",
            "grpc/reflection/v1alpha/reflection.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50051"
        ]
      },
      "endpoints": {
        "serverReflectionInfo": {}
      }
    },
    "service-ci": {
      "transports": {
        "grpc": {
          "service": "io.restorecommerce.commandinterface.Service",
          "protos": [
            "io/restorecommerce/commandinterface.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50051"
        ]
      },
      "endpoints": {
        "reset": {},
        "restore": {},
        "check": {},
        "version": {}
      }
    }
  },
  "database": {
    "main": {
      "provider": "arango",
      "host": "0.0.0.0",
      "port": 8529,
      "database": "identity-srv-test",
      "autoCreate": true,
      "retries": 6,
      "delay": 30000,
      "collections": [
        "users",
        "roles"
      ]
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "restore-identity-srv-test",
      "clientId": "restore-identity-srv-test",
      "connectionString": "0.0.0.0:2181",
      "latestOffset": true,
      "usersModified": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.User"
      },
      "usersCreated": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.User"
      },
      "usersDeleted": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.User"
      },
      "rolesCreated": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Role"
      },
      "rolesModified": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Role"
      },
      "rolesDeleted": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Deleted"
      },
      "registered": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.User"
      },
      "activated": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.Activate"
      },
      "passwordChanged": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.PasswordChanged"
      },
      "emailIdChanged": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.EmailIdChanged"
      },
      "unregistered": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.Deleted"
      },
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "renderRequest": {
        "protos": [
          "io/restorecommerce/rendering.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.rendering.RenderRequest"
      },
      "renderResponse": {
        "protos": [
          "io/restorecommerce/rendering.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.rendering.RenderResponse"
      },
      "sendEmail": {
        "protos": [
          "io/restorecommerce/notification.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.notification.Notification"
      },
      "topics": {
        "users.resource": {
          "topic": "io.restorecommerce.users.resource"
        },
        "roles.resource": {
          "topic": "io.restorecommerce.roles.resource"
        },
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "healthCheckCommand",
            "restoreCommand",
            "resetStartCommand"
          ]
        },
        "rendering": {
          "topic": "io.restorecommerce.rendering",
          "events": [
            "renderResponse"
          ]
        }
      }
    },
    "enableEvents": "true"
  },
  "server": {
    "services": {
      "io-restorecommerce-user-srv": {
        "activate": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "changePassword": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "changeEmailId": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "find": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "register": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "unregister": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "login": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "create": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "read": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "delete": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "update": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-role-srv": {
        "read": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "create": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "modify": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "delete": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-identity-reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-identity-cis": {
        "reconfigure": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "reset": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "restore": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "check": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "version": {
          "transport": [
            "grpcIdentity"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpcIdentity",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-user-srv": "io.restorecommerce.user.Service",
          "io-restorecommerce-role-srv": "io.restorecommerce.role.Service",
          "io-restorecommerce-identity-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "io-restorecommerce-identity-cis": "io.restorecommerce.commandinterface.Service"
        },
        "protos": [
          "io/restorecommerce/user.proto",
          "io/restorecommerce/role.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50051"
      }
    ]
  },
  "serviceNames": {
    "user": "io-restorecommerce-user-srv",
    "role": "io-restorecommerce-role-srv",
    "reflection": "io-restorecommerce-identity-reflection",
    "cis": "io-restorecommerce-identity-cis"
  }
}
