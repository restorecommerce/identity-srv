{
  "service": {
    "userActivationRequired": true,
    "register": true
  },
  "client": {
    "hbs_templates": {
      "url": true,
      "prefix": "http://localhost:8080/",
      "templates": {
        "subject": "restore_identity/subject.hbs",
        "body_register": "restore_identity/body_register.hbs",
        "body_change": "restore_identity/body_change.hbs",
        "layout": "restore_identity/layout.hbs"
      }
    }
  },
  "database": {
    "main": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "identity-srv",
      "autoCreate": true,
      "retries": 6,
      "delay": 30000,
      "collections": [
        "users",
        "roles"
      ]
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "restore-identity-srv",
      "clientId": "restore-identity-srv",
      "connectionString": "localhost:2181",
      "usersCreated": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.UserList"
      },
      "usersModified": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.UserList"
      },
      "usersDeleted": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.Deleted"
      },
      "rolesCreated": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Role"
      },
      "rolesModified": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Role"
      },
      "rolesDeleted": {
        "protos": [
          "io/restorecommerce/role.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.role.Deleted"
      },
      "registered": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.User"
      },
      "activated": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.Activate"
      },
      "passwordChanged": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.PasswordChanged"
      },
      "emailIdChanged": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.EmailIdChanged"
      },
      "unregistered": {
        "protos": [
          "io/restorecommerce/user.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.user.Deleted"
      },
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "renderRequest": {
        "protos": [
          "io/restorecommerce/rendering.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.rendering.RenderRequest"
      },
      "identityRenderResponse": {
        "protos": [
          "io/restorecommerce/rendering.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.rendering.RenderResponse"
      },
      "sendEmail": {
        "protos": [
          "io/restorecommerce/notification.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.notification.Notification"
      },
      "topics": {
        "users.resource": {
          "topic": "io.restorecommerce.users.resource"
        },
        "roles.resource": {
          "topic": "io.restorecommerce.roles.resource"
        },
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "healthCheckCommand",
            "restoreCommand",
            "resetCommand",
            "versionCommand"
          ]
        },
        "rendering": {
          "topic": "io.restorecommerce.rendering",
          "events": [
            "identityRenderResponse"
          ]
        },
        "notification": {
          "topic": "io.restorecommerce.notification"
        }
      }
    },
    "enableEvents": "true"
  },
  "server": {
    "services": {
      "io-restorecommerce-user-srv": {
        "activate": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "changePassword": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "changeEmailId": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "find": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "register": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "unregister": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "verifyPassword": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "createUsers": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "read": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-role-srv": {
        "read": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "create": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "update": {
          "transport": [
            "grpcIdentity"
          ]
        },
		    "upsert": {
          "transport": [
            "grpcIdentity"
          ]
        },
        "delete": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-identity-reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpcIdentity"
          ]
        }
      },
      "io-restorecommerce-identity-cis": {
        "command": {
          "transport": [
            "grpcIdentity"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpcIdentity",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-user-srv": "io.restorecommerce.user.Service",
          "io-restorecommerce-role-srv": "io.restorecommerce.role.Service",
          "io-restorecommerce-identity-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "io-restorecommerce-identity-cis": "io.restorecommerce.commandinterface.Service"
        },
        "protos": [
          "io/restorecommerce/user.proto",
          "io/restorecommerce/role.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50051"
      }
    ]
  },
  "serviceNames": {
    "user": "io-restorecommerce-user-srv",
    "role": "io-restorecommerce-role-srv",
    "reflection": "io-restorecommerce-identity-reflection",
    "cis": "io-restorecommerce-identity-cis"
  }
}
